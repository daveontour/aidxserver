				</xs:annotation>
			</xs:element>
			<xs:element name="Route" type="aip:Route" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flight routing information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Codeshares" type="aip:CodeshareCollection" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information of flights which code-share with the flight leg</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResourceAllocations" type="aip:ResourceAllocationCollection" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of all airport resources associated with flight.

NB: ResourceAllocation elements will appear sorted in ascending order based on the TimeSlot/Start value</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightLoads" type="aip:FlightLoadCollection" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of flight load information e.g. pax, freight etc</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightServices" type="aip:FlightServiceCollection" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of flight service information e.g. ground-power consumption, pre-conditioned air, de-icing etc. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightIrregularityDelays" type="aip:FlightIrregularityDelayCollection" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of flight irregularity delays</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Terminal" type="aip:Terminal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Airport Terminal Code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LinkedFlightID" type="aip:FlightID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identification information of the flight linked to this flight leg</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightHandlers" type="aip:FlightHandlerCollection" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection of flight operations handler information records</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Exception" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates flight exception information e.g. Military, Ambulance, Presidential etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Transfers" type="aip:TransferCollection" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information of flights with passengers transferring to/from this flight leg</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PassengerServices" type="aip:PassengerServiceCollection" minOccurs="0"/>
			<xs:element name="CustomFields" type="aip:AIPFlightDataCustomFields" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides support for extending schema with custom field definitions</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightDataCollection">
		<xs:annotation>
			<xs:documentation>Collection of FlightData elements</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlightData" type="aip:FlightData" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightDataCondition">
		<xs:annotation>
			<xs:documentation>Structure containing elements that allow specifying conditional criteria for requesting flight data</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlightIDCondition" type="aip:FlightIDCondition" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies conditions for various flight ID elements</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ServiceTypeCondition" type="aip:FlightServiceTypeCondition" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies conditions for matching against flight service type codes</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InternationalStatusCondition" type="aip:InternationalStatusCondition" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Structure which supports specifying conditions on flight international status</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightEventConditions" type="aip:FlightEventConditionCollection" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies required flight event conditions which Flight data will be selected against</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RouteCondition" type="aip:RouteCondition" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies flight route conditional information which Flight data will be selected against</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResourceConditions" type="aip:ResourceAllocationConditionCollection" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies flight resource allocation information which Flight data will be selected against</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TerminalCondition" type="aip:TerminalCondition" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies airport terminal information which Flight data will be selected against</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AssociatedFlightLegCondition" type="aip:AssociatedFlightLegCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies conditions for filtering on the information of the flight's associated flight leg</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrimaryAircraftCondition" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RegistrationCondition" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Equal" type="xs:string"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="CustomFieldCondition" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="aip:GenericConditionalFieldValue">
							<xs:attribute name="FieldName" use="required"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="CodeshareConditions" type="aip:CodeshareConditionCollection" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightEvent">
		<xs:annotation>
			<xs:documentation>Represents a single flight event record</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EventCode">
				<xs:annotation>
					<xs:documentation>Event type identifier code.
Refer to PADIS Codes List elements 2005 and 9750</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="CodeContext" type="aip:IATA_PADIS_FlightEventCodeContext"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:choice>
				<xs:element name="DateTime" type="xs:dateTime" nillable="true">
					<xs:annotation>
						<xs:documentation>Event date and time including time zone information.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PartialDateTime" type="aip:PartialDateTime"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="Index" type="xs:unsignedInt">
			<xs:annotation>
				<xs:documentation>Provides a mechanism whereby elements in a collection could be uniquely identiefied and referenced</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="FlightEventCodeCondition">
		<xs:annotation>
			<xs:documentation>Defines filter conditions based on flight event code</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="Equal">
				<xs:annotation>
					<xs:documentation>Flight event code value</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="aip:IATA_CodeType">
							<xs:attribute name="CodeContext" type="aip:IATA_PADIS_FlightEventCodeContext" use="required">
								<xs:annotation>
									<xs:documentation>Indicates the PADIS Code List which the specified code applies to</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="NotExist">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="aip:IATA_CodeType">
							<xs:attribute name="CodeContext" type="aip:IATA_PADIS_FlightEventCodeContext" use="required">
								<xs:annotation>
									<xs:documentation>Indicates the PADIS Code List which the specified code applies to</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="FlightEventCollection">
		<xs:annotation>
			<xs:documentation>Collection of FlightEvent records</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlightEvent" type="aip:FlightEvent" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightEventCondition">
		<xs:annotation>
			<xs:documentation>Structure which supports specifying a condition for a flight event</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EventCode" type="aip:FlightEventCodeCondition">
				<xs:annotation>
					<xs:documentation>Event code condition</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateTime" type="aip:DateTimeCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Event date and time condition</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightEventConditionCollection">
		<xs:annotation>
			<xs:documentation>Collection of FlightEventCondition elements</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlightEventCondition" type="aip:FlightEventCondition" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A single flight event condition</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightHandler">
		<xs:sequence>
			<xs:element ref="aip:Company"/>
			<xs:element name="HandlerType" type="aip:FlightHandlerTypeType">
				<xs:annotation>
					<xs:documentation>Handler operational area</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightHandlerCollection">
		<xs:annotation>
			<xs:documentation>Collection of FlightEvent records</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlightHandler" type="aip:FlightHandler" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Flight operation handler</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightID">
		<xs:annotation>
			<xs:documentation>Structure containing elements which uniquely identify a flight record</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Specifies values for the primary flight identifier fields i.e. airline code, flight number, STO, flight nature and airport required to either match an existing flight or create a new flight.</xs:documentation>
				</xs:annotation>
				<xs:element ref="aip:Airport"/>
				<xs:element name="Airline" type="aip:Company">
					<xs:annotation>
						<xs:documentation>Container for airline information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="aip:FlightNumber"/>
				<xs:element ref="aip:FlightSuffix" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Flight number suffix</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="STO" type="aip:STO">
					<xs:annotation>
						<xs:documentation>Flight scheduled  date and time including time zone information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="aip:FlightNature">
					<xs:annotation>
						<xs:documentation>Flight leg nature</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SourceUniqueID" type="aip:SourceUniqueIDType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Unique flight record identifier in the context of the sending system</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AirportOperationsUniqueID" type="xs:string" minOccurs="0"/>
			</xs:sequence>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>Flight identifier structure intended to match existing flight records, identifying such records by means of a unique record ID issued by the AIE and shared with external systems who can in turn include the same ID in data sent back to the AIE for the corresponding flight record.</xs:documentation>
				</xs:annotation>
				<xs:element name="AIPUniqueID" type="aip:AIPUniqueIDType">
					<xs:annotation>
						<xs:documentation>Unique flight record identifier in the context of the AIP</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SourceUniqueID" type="aip:SourceUniqueIDType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Unique flight record identifier in the context of the sending system</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Airline" type="aip:Company" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Cotnainer for airline information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="aip:FlightNumber" minOccurs="0"/>
				<xs:element ref="aip:FlightSuffix" minOccurs="0"/>
				<xs:element name="STO" type="aip:STO" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Flight scheduled  date and time including time zzone information</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="aip:Airport" minOccurs="0"/>
				<xs:element ref="aip:FlightNature" minOccurs="0"/>
				<xs:element name="AirportOperationsUniqueID" type="xs:string" minOccurs="0"/>
			</xs:sequence>
			<xs:sequence>
				<xs:annotation>
					<xs:documentation>A third option providing a more flexible method for specifying criteria for searching and matching an existing flight record</xs:documentation>
				</xs:annotation>
				<xs:element name="FlightUpdateMatchConditions" type="aip:FlightIDConditions">
					<xs:annotation>
						<xs:documentation>Provides an alternative method for specifying the criteria whereby an existing flight will be searched for.  Rather than specifying fixed, specific flight identifier values it allows specifying flexible search criteria for finding the flight which the message payload information will be applied to</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FlightCreateKeyElements" type="aip:FlightCreateKeys" minOccurs="0"/>
			</xs:sequence>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="FlightIDCondition">
		<xs:annotation>
			<xs:documentation>Contains elements allowing criteria to be specified on the data fields which uniquely identify a flight record</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AIPUniqueIDCondition" type="aip:AIPUniqueIdCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Conditions applied to the unique flight record ID in the context of the AIP</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirlineCondition" type="aip:AirlineCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition applied to the flight airline code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightNumberCondition" type="aip:FlightNumberCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition applied to the flight number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightSuffixCondition" type="aip:FlightNumberSuffixCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition applied to the flight number suffix</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STOCondition" type="aip:DateTimeCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition applied to the flight STO date and time</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STODateCondition" type="aip:DateCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition applied to the flight STO date component</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightNatureCondition" type="aip:FlightNatureCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition applied to the flight nature</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AirportCondition" type="aip:AirportCodeCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition applied to local airport code</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightIDConditions">
		<xs:sequence>
			<xs:element name="UniqueFlightMatchCondition" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Container for a set of conditions used for searching flight records.  In the case where a set of multiple flights are matched based on the specified criteria the first match in the set will be accepted to be the correct record and updated, certain data elements provide support for ordering the data set in a manner which will allow the most likely candidate record to appear as the first record in the matched set</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="aip:FlightMatchCondition">
							<xs:attribute name="Priority" type="xs:integer" use="required">
								<xs:annotation>
									<xs:documentation>Assigns a priority to multiple UniqueFlightMatchCondition elements.  These are processed in order of this priority and the first succesfull unique match will be processed.
A value of 1 has highest priority with priority decreasing as the value increases</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightIrregularity">
		<xs:annotation>
			<xs:documentation>Represents a single flight irregularity delay record</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Code">
				<xs:annotation>
					<xs:documentation>Reason for delay as per IATA AHM 730</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CustomCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Non-IATA custom delay code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Suffix" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Suffix code</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Duration" type="xs:duration" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Delay Duration</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Cause" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Free-text description of the cause of the delay</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Index" type="xs:integer" use="required">
			<xs:annotation>
				<xs:documentation>Provides a mechanism whereby elements in a collection could be uniquely identified and referenced</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="FlightIrregularityDelayCollection">
		<xs:annotation>
			<xs:documentation>Collection of FlightIrregularityDelay records</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlightIrregularityDelay" type="aip:FlightIrregularity" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A flight delay</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightLoad">
		<xs:annotation>
			<xs:documentation>Represents a single flight load record</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Code">
				<xs:annotation>
					<xs:documentation>Load type code</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="CodeContext" type="aip:IATA_PADIS_LoadCodeContext" use="required">
								<xs:annotation>
									<xs:documentation>Specifies the PADIS code set (or custom) context of a flight load code</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Quantity">
				<xs:annotation>
					<xs:documentation>Load quantity</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:decimal">
							<xs:attribute name="UnitOfMeasure" type="aip:WeightUnit"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Index" type="xs:integer">
			<xs:annotation>
				<xs:documentation>Provides a mechanism whereby elements in a collection could be uniquely identiefied and referenced</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="FlightLoadCollection">
		<xs:annotation>
			<xs:documentation>Collection of FlightLoad records</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlightLoad" type="aip:FlightLoad" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A flight load</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightMatchCondition">
		<xs:annotation>
			<xs:documentation>Contains elements allowing criteria to be specified on the data fields which uniquely identify a flight record</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirlineCondition" type="aip:AirlineCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition applied to the flight airline code</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightNumberCondition" type="aip:FlightNumberCondition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condition applied to the flight number</xs:documentation>
				</xs:annotation>
			</xs:element>
